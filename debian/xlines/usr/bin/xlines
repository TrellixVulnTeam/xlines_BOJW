#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'xlines==0.7.4','console_scripts','xlines'
__requires__ = 'xlines==0.7.4'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('xlines==0.7.4', 'console_scripts', 'xlines')()
    )
